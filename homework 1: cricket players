cricket_players = []


cricket_players.append("ajinkya")
cricket_players.append("virat")
cricket_players.append("jadeja")
cricket_players.append("bumrah")
cricket_players.append("bhuvneshvar")
print(cricket_players)


for players in cricket_players:
  if players == "bhuvneshvar":
    print("bhuvneshvar found")
  else:
    print("not found")  

